<?php

namespace App\Models;

use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Log extends Model
{
    use HasFactory;

    protected $fillable = [
        'user_id',
        'action',
        'model_type',
        'model_id',
        'old_values',
        'new_values',
        'created_at',
        'updated_at',
        'finished_at',
        'started_at',
    ];

    protected $casts = [
        'finished_at' => 'datetime',
        'started_at' => 'datetime',
    ];
}